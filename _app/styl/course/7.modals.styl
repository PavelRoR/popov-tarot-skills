.white-popup
	position: relative;
	background: #FFF;
	padding: 20px 140px 20px 20px
	width: auto;
	max-width: 500px;
	margin: 20px auto;
	// background-image url(../img/icons/card-pentacle.png)
	// background-repeat no-repeat
	// background-position: center right 20px
	// background-size: 100px
.general-img-modal
	width 100px
	top 30%
	right 20px
.modal-title
	font-size: 20px
	font-family: SUDB
	font-weight: bold
	color cB
	margin-bottom: 10px
	text-transform uppercase
.mfp-fade.mfp-bg
	opacity: 0;
	-webkit-transition: all 0.45s ease-out;
	-moz-transition: all 0.45s ease-out;
	transition: all 0.45s ease-out;
/* overlay animate in */
.mfp-fade.mfp-bg.mfp-ready
	opacity: 0.9;
/* overlay animate out */
.mfp-fade.mfp-bg.mfp-removing
	opacity: 0;

/* content at start */
.mfp-fade.mfp-wrap .mfp-content
	opacity: 0;
	-webkit-transition: all 0.45s ease-out;
	-moz-transition: all 0.45s ease-out;
	transition: all 0.45s ease-out;
/* content animate it */
.mfp-fade.mfp-wrap.mfp-ready .mfp-content
	opacity: 1;

/* content animate out */
.mfp-fade.mfp-wrap.mfp-removing .mfp-content
	opacity: 0;